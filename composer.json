{
  "name": "piedweb/piedweb",
  "replace": {
    "piedweb/curl": "self.version"
  },
  "description": "Monorepo for PiedWeb's PHP Package.",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "babdev/pagerfanta-bundle": "^3.7",
    "danielstjules/stringy": "^3.1",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.13",
    "jeremykendall/php-domain-parser": "^6.1",
    "league/csv": "^9.8",
    "league/flysystem": "^3.5",
    "league/uri": "^6.5",
    "neitanod/forceutf8": "^2.0",
    "nesk/puphpeteer": "dev-zoon",
    "pagerfanta/doctrine-orm-adapter": "^3.6",
    "pagerfanta/twig": "^3.6",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.8",
    "sensio/framework-extra-bundle": "^6.1",
    "sonata-project/doctrine-orm-admin-bundle": "^4.7",
    "spatie/robots-txt": "^2.0",
    "symfony/asset": "^6.0",
    "symfony/cache": "^6.0",
    "symfony/console": "^6.0",
    "symfony/css-selector": "^6.0",
    "symfony/doctrine-messenger": "^6.0",
    "symfony/dom-crawler": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/expression-language": "^6.0",
    "symfony/flex": "^2",
    "symfony/form": "^6.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/http-client": "^6.0",
    "symfony/intl": "^6.0",
    "symfony/mailer": "^6.0",
    "symfony/mime": "^6.0",
    "symfony/monolog-bundle": "^3.8",
    "symfony/notifier": "^6.0",
    "symfony/process": "^6.0",
    "symfony/property-access": "^6.0",
    "symfony/property-info": "^6.0",
    "symfony/proxy-manager-bridge": "^6.0",
    "symfony/runtime": "^6.0",
    "symfony/security-bundle": "^6.0",
    "symfony/serializer": "^6.0",
    "symfony/string": "^6.0",
    "symfony/translation": "^6.0",
    "symfony/twig-bundle": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/web-link": "^6.0",
    "symfony/webpack-encore-bundle": "^1.15",
    "symfony/yaml": "^6.0",
    "thecodingmachine/safe": "^2.1",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/intl-extra": "^3.4",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "brianium/paratest": "^6.4",
    "friendsofphp/php-cs-fixer": "^3.2",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.14",
    "symfony/browser-kit": "^6.0",
    "symfony/debug-bundle": "^6.0",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/stopwatch": "^6.0",
    "symfony/var-dumper": "^6.0",
    "symfony/web-profiler-bundle": "^6.0",
    "symplify/monorepo-builder": "^11.1",
    "vimeo/psalm": "^4",
    "vincentlanglet/twig-cs-fixer": "^0.5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "runtime": {
      "dotenv_path": "packages/seo-status/.env"
    }
  },
  "autoload": {
    "psr-4": {
      "PiedWeb\\Curl\\": "packages/curl/src",
      "PiedWeb\\Google\\": "packages/google/src",
      "PiedWeb\\GoogleSpreadsheetSeoScraper\\": "packages/google-spreadsheet-seo-scraper/src",
      "PiedWeb\\Extractor\\": "packages/extractor/src",
      "PiedWeb\\SeoPocketCrawler\\": "packages/seo-pocket-crawler/src",
      "PiedWeb\\SeoStatus\\": "packages/seo-status/src",
      "PiedWeb\\TextAnalyzer\\": "packages/text-analyzer/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PiedWeb\\Curl\\Test\\": "packages/curl/tests",
      "PiedWeb\\Google\\Test\\": "packages/google/tests",
      "PiedWeb\\Extractor\\Test\\": "packages/extractor/tests",
      "PiedWeb\\SeoPocketCrawler\\Test\\": "packages/seo-pocket-crawler/tests",
      "PiedWeb\\SeoStatus\\Test\\": "packages/seo-status/tests",
      "PiedWeb\\TextAnalyzer\\Test\\": "packages/text-analyzer/tests"
    }
  },
  "scripts": {
    "stan": "vendor/bin/phpstan analyze packages/*/src",
    "psalm": "vendor/bin/psalm --no-suggestions --no-cache",
    "rector": "vendor/bin/rector process",
    "test": "vendor/bin/phpunit --stop-on-failure",
    "test-seo-status": "vendor/bin/phpunit --stop-on-failure --testsuite seo-status",
    "test-google": "vendor/bin/phpunit --stop-on-failure --testsuite google",
    "paratest": "vendor/bin/paratest -p16",
    "format": "vendor/bin/php-cs-fixer fix",
    "format-twig": "vendor/bin/twig-cs-fixer lint --fix ./",
    "reset-db": [
      "Composer\\Config::disableProcessTimeout",
      "bash packages/seo-status/script/reset-database"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": false,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/zoonru/puphpeteer.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/zoonru/rialto.git"
    }
  ]
}
